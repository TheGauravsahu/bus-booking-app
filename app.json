{
  "expo": {
    "name": "bus-booking-app",
    "slug": "bus-booking-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "busbookingapp",
    "icon": "./assets/images/logo.jpeg",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
